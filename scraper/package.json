{
  "name": "scraper",
  "displayName": "scraper-AI",
  "icon": "media/icon.png",
  "description": "",
  "version": "1.0.6",
  "publisher": "Christine-devops1234",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:scraper.chatView",
    "onCommand:scraper.scrapeForCode",
    "onCommand:scraper.semanticSearch",
    "onCommand:scraper.uploadProjectToAgent"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "scraper.scrapeForCode",
        "title": "Scrape for code"
      },
      {
        "command": "scraper.semanticSearch",
        "title": "Semantic File Search"
      },
      {
        "command": "scraper.uploadProjectToAgent",
        "title": "Upload Project to Agent"
      }
    ],
    "viewsContainers": {},
    "views": {
      "panel": [
        {
          "type": "webview",
          "id": "scraper.chatView",
          "name": "Scraper Chat"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.101.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "crypto": "^1.0.1",
    "fs": "^0.0.1-security",
    "marked": "^16.0.0",
    "path": "^0.12.7"
  }
}
